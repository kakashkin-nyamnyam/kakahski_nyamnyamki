budget = input("Введите бюджет: ")
platform = input("Введите платформу: ")

if budget >= '50000':
    print("Крупная кампания")
    if platform == "Instagram":
        print("Запускаем stories и reels")
        strategy = "influencer"
    else:
        print("Выбираем другой формат")
        strategy = "standard"
else:
    print("Ограниченный бюджет")
    strategy = "organic"
